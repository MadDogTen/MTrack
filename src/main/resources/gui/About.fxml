    <children>
        <ImageView fitHeight="208.0" fitWidth="189.0" opacity="0.38">
            <image>
                <Image url="@../image/MTrackLogo.png"/>
            </image>
        </ImageView>
        <Label fx:id="programName" layoutX="47.0" layoutY="8.0" prefHeight="34.0" prefWidth="102.0" text="MTrack" textAlignment="CENTER">
