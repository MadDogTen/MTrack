        </Button>
        <Button fx:id="minimize" layoutX="232.0" layoutY="2.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="23.0">
            <graphic>
                <ImageView fitHeight="12.0" fitWidth="7.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../image/UI/MinimiseButton.png"/>
                    </image>
                </ImageView>
