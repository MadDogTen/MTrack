<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" id="pane" fx:id="pane" prefHeight="454.0" prefWidth="281.0" stylesheets="@MainGui.css" xmlns="http://javafx.com/javafx/8" fx:controller="com.maddogten.mtrack.Controller">
