                                        text="Add Directory"/>
                                <Button fx:id="removeDirectory" layoutX="121.0" layoutY="27.0" mnemonicParsing="false"
                                        text="Remove Directory"/>
                                <Button fx:id="setDirectoryTimeout" layoutX="125.0" layoutY="103.0"
                                        mnemonicParsing="false" text="Set"/>
                                <TextField fx:id="directoryTimeoutTextField" layoutX="79.0" layoutY="103.0"
                                           prefHeight="25.0" prefWidth="36.0"/>
                                <Text fx:id="directoryTimeoutText" layoutX="45.0" layoutY="91.0" strokeType="OUTSIDE"
                                      strokeWidth="0.0" text="Directory Timeout (Seconds):"/>
