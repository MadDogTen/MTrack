            <graphic>
                <ImageView fitHeight="12.0" fitWidth="7.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../image/UI/ExitButton.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</Pane>
