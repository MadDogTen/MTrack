<?xml version="1.0" encoding="UTF-8"?>

<!--suppress JavaFxDefaultTag -->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
      fx:controller="program.FXMLControllers.Settings">
    <children>
        <TabPane fx:id="tabPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                 prefHeight="244.0" prefWidth="213.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="mainTab" closable="false" text="Main">
                    <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="forceRecheck" layoutX="42.0" layoutY="14.0" mnemonicParsing="false"
                                text="Force Recheck Shows"/>
                        <Button fx:id="changeUpdateSpeed" layoutX="43.0" layoutY="46.0" mnemonicParsing="false"
                                text="Change Update Time"/>
                        <Button fx:id="about" layoutX="83.0" layoutY="188.0" mnemonicParsing="false" text="About"/>
                    </children>
                </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="usersTab" closable="false" text="Users">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="371.0" prefWidth="180.0">
                            <children>
                                <Button fx:id="setDefaultUsername" layoutX="27.0" layoutY="31.0" mnemonicParsing="false"
                                        text="Set Default user"/>
                                <Button fx:id="clearDefaultUsername" layoutX="129.0" layoutY="31.0"
                                        mnemonicParsing="false" text="Reset"/>
                                <Button fx:id="deleteUser" layoutX="94.0" layoutY="60.0" mnemonicParsing="false"
                                        text="Delete User"/>
                                <Button fx:id="addUser" layoutX="27.0" layoutY="60.0" mnemonicParsing="false"
                                        text="Add User"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="otherTab" closable="false" text="Other">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Button fx:id="openProgramFolder" layoutX="42.0" layoutY="176.0" mnemonicParsing="false"
                                        text="Open Settings Folder"/>
                                <Button fx:id="addDirectory" layoutX="4.0" layoutY="14.0" mnemonicParsing="false"
                                        text="Add Directory"/>
                                <Button fx:id="removeDirectory" layoutX="98.0" layoutY="14.0" mnemonicParsing="false"
                                        text="Remove Directory"/>
                                <Button fx:id="changeLanguage" layoutX="41.0" layoutY="63.0" mnemonicParsing="false"
                                        text="Change Language"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="developerTab" text="Developer">
                    <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TabPane prefHeight="216.0" prefWidth="213.0" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                                <Tab fx:id="dev1Tab" closable="false" text="Dev 1">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="printAllShows" layoutX="2.0" layoutY="2.0"
                                                        mnemonicParsing="false" text="Print All Show Info"/>
                                                <Button fx:id="printAllDirectories" layoutX="2.0" layoutY="29.0"
                                                        mnemonicParsing="false" text="Print All Directories"/>
                                                <Button fx:id="printEmptyShowFolders" layoutX="2.0" layoutY="56.0"
                                                        mnemonicParsing="false" text="Print Empty Shows"/>
                                                <Button fx:id="setAllActive" layoutX="2.0" layoutY="159.0"
                                                        mnemonicParsing="false" text="Set All Active"/>
                                                <Button fx:id="setAllInactive" layoutX="90.0" layoutY="159.0"
                                                        mnemonicParsing="false" text="Set All Inactive"/>
                                                <Button fx:id="printIgnoredShows" layoutX="2.0" layoutY="83.0"
                                                        mnemonicParsing="false" text="Print Ignored Shows"/>
                                                <Button fx:id="printHiddenShows" layoutX="2.0" layoutY="110.0"
                                                        mnemonicParsing="false" text="Print Hidden Shows"/>
                                                <Button fx:id="unHideAll" layoutX="124.0" layoutY="110.0"
                                                        mnemonicParsing="false" prefHeight="25.0" prefWidth="73.0"
                                                        text="UnHide All" wrapText="true"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="dev2Tab" closable="false" text="Dev 2">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="printProgramSettingsFileVersion" layoutX="2.0"
                                                        layoutY="2.0" mnemonicParsing="false" text="Print PSFV"/>
                                                <Button fx:id="printUserSettingsFileVersion" layoutX="2.0"
                                                        layoutY="29.0" mnemonicParsing="false" text="Print USFV"/>
                                                <Button fx:id="clearFile" layoutX="146.0" layoutY="112.0"
                                                        mnemonicParsing="false" text="Clear File"/>
                                                <Button fx:id="deleteEverythingAndClose" layoutX="117.0" layoutY="159.0"
                                                        mnemonicParsing="false" text="Reset Program"/>
                                                <Button fx:id="printAllUserInfo" layoutX="2.0" layoutY="58.0"
                                                        mnemonicParsing="false" text="Print All User Info"/>
                                                <Button fx:id="add1ToDirectoryVersion" layoutX="2.0" layoutY="112.0"
                                                        mnemonicParsing="false" text="Directory Version +1"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Button fx:id="exit" layoutX="188.0" layoutY="2.0" mnemonicParsing="false" text="X"/>
    </children>
</Pane>
