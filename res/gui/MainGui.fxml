<?xml version="1.0" encoding="UTF-8"?>

<!--suppress JavaFxDefaultTag -->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="pane" xmlns="http://javafx.com/javafx/8"
      fx:controller="program.Controller">
    <children>
        <TabPane fx:id="tabPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                 prefHeight="454.0" prefWidth="282.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="showsTab" closable="false" text="Shows">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="tableView" layoutY="40.0" prefHeight="385.0" prefWidth="281.0">
                                    <columns>
                                        <TableColumn fx:id="shows" editable="false" prefWidth="214.0" text="Shows"/>
                                        <TableColumn fx:id="remaining" editable="false" minWidth="0.0" prefWidth="52.0"
                                                     text="Left"/>
                                    </columns>
                                </TableView>
                                <TextField fx:id="textField" layoutX="8.0" layoutY="6.0"/>
                                <CheckBox fx:id="show0RemainingCheckBox" layoutX="168.0" layoutY="10.0"
                                          mnemonicParsing="false"/>
                                <MenuButton fx:id="menuButton" layoutX="200.0" layoutY="6.0" mnemonicParsing="false"
                                            prefHeight="25.0" prefWidth="74.0" text="Options">
                                    <items>
                                        <MenuItem fx:id="changeTableView" mnemonicParsing="false"
                                                  text="Switch between Active/Inactive List"/>
                                        <MenuItem fx:id="viewChanges" mnemonicParsing="false"
                                                  text="Open Changes Window"/>
                                    </items>
                                </MenuButton>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="settingsTab" closable="false" text="Settings">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Button fx:id="exit" layoutX="257.0" layoutY="2.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="22.0"
                text="X" textAlignment="CENTER" textOverrun="CLIP"/>
        <Button fx:id="minimize" layoutX="232.0" layoutY="2.0" mnemonicParsing="false" prefHeight="25.0"
                prefWidth="23.0" text="â€“"/>
        <ProgressIndicator id="isCurrentlyRechecking" fx:id="isCurrentlyRechecking" layoutX="145.0" layoutY="2.0"
                           prefHeight="42.0" prefWidth="56.0" progress="0.0" stylesheets="@MainGui.css"
                           visible="false"/>
        <Button fx:id="changesAlert" layoutX="190.0" layoutY="2.0" mnemonicParsing="false" prefHeight="1.0"
                prefWidth="1.0" text="!" visible="false"/>
        <Circle fx:id="pingingDirectory" fill="#2deb30" layoutX="173.0" layoutY="14.0" radius="8.0" stroke="BLACK"
                strokeType="INSIDE" strokeWidth="0.3" visible="false"/>
    </children>
</Pane>
